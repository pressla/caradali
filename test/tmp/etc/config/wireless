
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/ar933x_wmac'
	option htmode 'HT20'
	option country 'DE'
	option disabled '0'
	option txpower '20'
	option channel '9'

config wifi-iface 'ap'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option encryption 'none'
	option ifname 'apif'
	option ssid 'AIMLED'

config wifi-iface 'mesh'
	option device 'radio0'
	option encryption 'none'
	option mode 'mesh'
	option network 'wan'
	option ifname 'meshif'
	option mesh_id 'mesh-test'
	option mesh_hwmp_rootmode '0'
	option mesh_gate_announcements '0'

